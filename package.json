{
  "name": "tyranoparser",
  "version": "0.0.0",
  "description": "[Experimental] Parse `TyranoScript` Scenario to `Unist` structure",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "workspaces": [
    "src/*"
  ],
  "private": true,
  "scripts": {
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src",
    "lint:fix": "eslint --fix src",
    "lint:check": "eslint --print-config src | eslint-config-prettier-check",
    "lint:diff": "eslint --print-config src | eslint-config-prettier-check",
    "lint:write": "eslint --fix src",
    "prettier": "prettier --write src/**/*.{ts,tsx}",
    "prettier:check": "prettier --check src/**/*.{ts,tsx}",
    "prettier:diff": "prettier --list-different src/**/*.{ts,tsx}",
    "prettier:fix": "prettier --write src/**/*.{ts,tsx}",
    "prettier:write": "prettier --write src/**/*.{ts,tsx}",
    "watch": "tsc src --out-dir dist --watch --source-maps",
    "build": "NODE_ENV=production tsc src -p tsconfig.production.ts --outDir dist --source-maps",
    "##": ""
  },
  "keywords": [
    "tyranoscript",
    "unist",
    "parser"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/botamochi0x12/tyranoparser.git"
  },
  "bugs": {
    "url": "https://github.com/botamochi0x12/tyranoparser/issues"
  },
  "homepage": "https://github.com/botamochi0x12/tyranoparser#readme",
  "author": "botamochi0x12 <botamochi0x12@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "@vitest/coverage-c8": "^0.29.8",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-standard-with-typescript": "^34.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.0",
    "jest": "^29.4.1",
    "prettier": "^2.8.3",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.4",
    "typescript-eslint": "0.0.1-alpha.0",
    "vitest": "^0.29.8"
  },
  "##": "##COMMENT",
  "dependencies": {
    "unist-builder": "^3.0.1"
  }
}
